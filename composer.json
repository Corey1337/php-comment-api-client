{
    "name": "corey/php-comment-api-client",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Corey\\PhpCommentApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corey\\PhpCommentApiClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Corey1337",
            "email": "kolikendr@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.5,<8.6",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.1",
        "php-http/discovery": "^1.20",
        "ext-uri": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "nyholm/psr7": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.93",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
